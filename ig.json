{
  "tool": "jekyll",
  "version": "4.0.1",
  "fixed-business-version": "1.1.0",
  "license": "CC-BY-SA-4.0",
  "npm-name": "ch.chmed20af.emediplan",
  "append-slash-to-dependency-urls": true,
  "paths": {
    "resources": [
      "resources"
    ],
    "pages": "pages",
    "temp": "temp",
    "output": "output",
    "qa": "qa",
    "txCache": "txCache",
    "specification": "http://hl7.org/fhir/r4"
  },
  "language": "en",
  "languages": [
    "en",
    "de-CH",
    "fr-CH",
    "it-CH"
  ],
  "path-pattern": "[type]-[id].html",
  "defaults": {
    "Any": {
      "xml": true,
      "json": true,
      "ttl": true,
      "xml-html": true,
      "json-html": true,
      "ttl-html": true,
      "template-format": "instance-template-format.html",
      "template-base": "instance-template-base.html"
    },
    "StructureDefinition": {
      "shex": true,
      "sch": true,
      "json-schema": true,
      "summary": true,
      "header": true,
      "diff": true,
      "snapshot": true,
      "pseudo-xml": true,
      "pseudo-json": true,
      "pseudo-ttl": true,
      "uml": true,
      "tx": true,
      "inv": true,
      "dict": true,
      "maps": true,
      "xref": true,
      "template-base": "instance-template-sd.html",
      "template-defns": "instance-template-sd-definitions.html",
      "template-mappings": "instance-template-sd-mappings.html",
      "template-examples": "instance-template-sd-examples.html"    
    },
    "ValueSet": {
      "template-base": "instance-template-vs.html"
    }
  },
  "canonicalBase": "http://chmed20af.emediplan.ch/fhir",
  "dependencyList": [
    {
        "name": "chcore",
        "package": "ch.fhir.ig.ch-core",
        "version": "current"
      },
      {
        "name": "cheprterm",
        "package": "ch.fhir.ig.ch-epr-term",
        "version": "current"
      },
      {      
        "name": "chemed",
        "package": "ch.fhir.ig.ch-emed",
        "version": "2.0.4"
    }
  ],
  "source": "implementationguide/ch.chmed20af.emediplan.xml",
  "sct-edition": "http://snomed.info/sct/900000000000207008",
  "resources": {},
  "extraTemplates": [
    {
      "name": "mappings",
      "description": "Mappings"
    },
    {
      "name": "examples",
      "description": "Examples"
    }
  ],
  "suppressedWarningFile": "suppressedwarnings.txt",
  "logging": [
    "init",
    "progress",
    "context",
    "html",
    "tx"
  ]
}