<StructureDefinition xmlns="http://hl7.org/fhir">
   <id value="chmed20af-analyzer-observation"/>
   <url value="http://chmed20af.emediplan.ch/fhir/StructureDefinition/chmed20af-analyzer-observation"/>
   <version value="0.1.0"/>
   <name value="AnalyzerObservation"/>
   <title value="Analyzer Observation"/>
   <status value="draft"/>
   <publisher value="IG eMediplan"/>
   <contact>
      <telecom>
         <system value="url"/>
         <value value="http://www.emediplan.ch"/>
      </telecom>
   </contact>
   <description value="Profile for the Analyzer Observation (MedicationPlan)"/>
   <fhirVersion value="4.0.1"/>
   <kind value="resource"/>
   <abstract value="false"/>
   <type value="Observation"/>
   <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Observation"/>
   <derivation value="constraint"/>
   <differential>
      <element id="Observation">
         <path value="Observation"/>
      </element>
      <element id="Observation.extension">
         <path value="Observation.extension"/>
         <slicing>
            <discriminator>
               <type value="value"/>
               <path value="url"/>
            </discriminator>
            <rules value="open"/>
         </slicing>
      </element>
      <element id="Observation.extension:treatmentPlan">
         <path value="Observation.extension" />
         <sliceName value="treatmentPlan" />
         <short value="Reference to the medication treatment plan"/>
         <min value="0"/>
         <max value="1"/>
         <type>
            <code value="Extension" />
            <profile value="http://fhir.ch/ig/ch-emed/StructureDefinition/ch-emed-ext-treatmentplan" />
         </type>
      </element>
      <element id="Observation.extension:prescription">
         <path value="Observation.extension" />
         <sliceName value="prescription" />
         <short value="Reference to the medication prescription"/>
         <min value="0"/>
         <max value="1"/>
         <type>
            <code value="Extension" />
            <profile value="http://fhir.ch/ig/ch-emed/StructureDefinition/ch-emed-ext-prescription" />
         </type>
      </element>
      <element id="Observation.extension:dispense">
         <path value="Observation.extension" />
         <sliceName value="dispense" />
         <short value="Reference to the medication dispense"/>
         <min value="0"/>
         <max value="1"/>
         <type>
            <code value="Extension" />
            <profile value="http://fhir.ch/ig/ch-emed/StructureDefinition/ch-emed-ext-dispense" />
         </type>
      </element>
      <element id="Observation.status">
         <path value="Observation.status"/>
         <short value="Status of the observation"/>
      </element>
       <element id="MedicationStatement.category">
         <path value="MedicationStatement.category"/>
      </element>
      <element id="MedicationStatement.category.coding">
         <path value="MedicationStatement.category.coding"/>
         <short value="ATC code if possible"/>
      </element>
      <element id="MedicationStatement.category.coding.system">
         <path value="MedicationStatement.category.coding.system"/>
         <fixedUri value="http://www.whocc.no/atc"/>
         <min value="1"/>
      </element>
      <element id="MedicationStatement.category.coding.code">
         <path value="MedicationStatement.category.coding.code"/>
         <min value="1"/>
      </element>
      <element id="MedicationStatement.category.text">
         <path value="MedicationStatement.category.text"/>
         <short value="Note that no ATC Code is applied"/>
      </element>
      <element id="Observation.code">
         <path value="Observation.code"/>
         <short value="Observation code"/>
         <binding>
            <strength value="required"/>
            <valueSet value="http://fhir.ch/ig/ch-emed/ValueSet/ihe-pharmaceuticaladvicestatuslist"/>
         </binding>
      </element>
      <element id="Observation.subject">
         <path value="Observation.subject"/>
         <short value="Patient"/>
         <type>
            <code value="Reference"/>
            <targetProfile value="http://chmed20af.emediplan.ch/fhir/StructureDefinition/chmed20af-rx-patient"/>
         </type>
         <min value="1"/>
      </element>
      <element id="Observation.performer">
         <path value="Observation.performer"/>
         <short value="Practitioner"/>
         <type>
            <code value="Reference"/>
            <targetProfile value="http://chmed20af.emediplan.ch/fhir/StructureDefinition/chmed20af-practitioner"/>
         </type>
         <min value="1"/>
         <max value="1"/>
      </element>
   </differential>
</StructureDefinition>